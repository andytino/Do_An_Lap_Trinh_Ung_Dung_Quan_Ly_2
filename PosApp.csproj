<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Assets\Icons\techShop.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Icons\close.png" />
    <None Remove="Assets\Icons\lock.png" />
    <None Remove="Assets\Icons\minus.png" />
    <None Remove="Assets\Icons\minusHover.png" />
    <None Remove="Assets\Icons\techShop.png" />
    <None Remove="Assets\Icons\techShop.svg" />
    <None Remove="Assets\Icons\user.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\Icons\techShop.ico" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Assets\Images\" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Icons\close.png" />
    <Resource Include="Assets\Icons\lock.png" />
    <Resource Include="Assets\Icons\minus.png" />
    <Resource Include="Assets\Icons\techShop.png">
      <CopyToOutputDirectory></CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\Icons\user.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Assets\Icons\techShop.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
