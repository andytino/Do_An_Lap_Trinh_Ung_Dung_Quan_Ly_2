<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
   
    
    <!--Background Colors-->
    <Color x:Key="primaryBackgroundColor">#0A003a</Color>
    <Color x:Key="secondaryBackgroundColor">#0A003a</Color>


    <!--Window Border Colors-->
    <Color x:Key="primaryWinBorderColor">#6D2FFF</Color>
    <Color x:Key="secondaryWinBorderColor">#FB539B</Color>
    <Color x:Key="thirdWinBorderColor">#836EFB</Color>
    <!--Color Palette -->
    
    <!--Font Colors-->
    <SolidColorBrush x:Key="primaryColor" Color="#0A003a"/>
    <SolidColorBrush x:Key="secondaryColor" Color="#0A003a"/>
    
    
    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#0A003a"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#0A003a"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#0A003a"/>
    
    
</ResourceDictionary>